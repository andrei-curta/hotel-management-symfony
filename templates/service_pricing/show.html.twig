{% extends 'base.html.twig' %}

{% block title %}ServicePricing{% endblock %}

{% block body %}
    <h1>ServicePricing</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ service_pricing.id }}</td>
            </tr>
            <tr>
                <th>StartDate</th>
                <td>{{ service_pricing.startDate ? service_pricing.startDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>EndDate</th>
                <td>{{ service_pricing.endDate ? service_pricing.endDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ service_pricing.price }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('service_pricing_index') }}">back to list</a>

    <a href="{{ path('service_pricing_edit', {'id': service_pricing.id}) }}">edit</a>

    {{ include('service_pricing/_delete_form.html.twig') }}
